<form action="/aids" style="margin-top: 7%;
margin-left: 30%;
margin-right: 30%;" method="POST" >
    <label for="a" style="margin: 1em 0 0 0;">Cd4 Cell: </label>
    <input type="text" id="cd4" name="cd4" placeholder="Cd4 Cell.....">
  
    <label for="b" style="margin: 1em 0 0 0;">Sweat: </label>
    <input type="text" id="sw" name="sw" placeholder="Sweat.....">
    
    <label for="c" style="margin: 1em 0 0 0;">Temperature: </label>
    <input type="text" id="temp" name="temp" placeholder="Temperature.....">
  
    <label for="d" style="margin: 1em 0 0 0;">Blood Pressure: </label>
    <input type="text" id="bp" name="bp" placeholder="Blood Pressure.....">
  
    <label for="e" style="margin: 1em 0 0 0;">Glucose Level: </label>
    <input type="text" id="glu" name="glu" placeholder="Glucose.....">
  
    <label for="f" style="margin: 1em 0 0 0;">Pain Level: </label>
    <input type="text" id="pl" name="pl" placeholder="Pain Level.....">
  
    <label for="g" style="margin: 1em 0 0 0;">Swelling: </label>
    <input type="text" id="swell" name="swell" placeholder="Swelling.....">
  
    <label for="h" style="margin: 1em 0 0 0;">Weakness Level: </label>
    <input type="text" id="wl" name="wl" placeholder="Weakness Level.....">
    
    <label for="i" style="margin: 1em 0 0 0;">Gender: </label>
    <input type="text" id="gender" name="gender" placeholder="Gender.....">

    <br>
    <input type="submit" value="Submit">
  </form>

  <script src="../../public/Drop.js"></script>
